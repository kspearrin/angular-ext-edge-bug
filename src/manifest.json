{
  "manifest_version": 2,
  "name": "Edge Test",
  "short_name": "Edge Test",
  "version": "1.25.7",
  "description": "Testing edge with angular.",
  "default_locale": "en",
  "author": "Me!",
  "homepage_url": "https://google.com",
  "icons": {
    "16": "images/icon16.png"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png"
    },
    "default_title": "Edge Test",
    "default_popup": "popup/index.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage",
    "clipboardWrite",
    "http://*/*",
    "https://*/*"
  ]
}
